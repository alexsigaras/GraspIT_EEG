<Window x:Class="GraspIT_EEG.DialogBoxes.TrainingCompleted"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Training Completed" Height="123" Width="285">
    <Grid Margin="0,0,-8,-7">
        <Button Width="100" Height="30" Content="Yes" IsDefault="True" Click="ButtonOkClick" Margin="14,0,0,13" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
        <Button Width="100" Height="30" Content="No" IsCancel="True" Margin="0,0,31,13" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
        <TextBlock Text="Training is completed." HorizontalAlignment="Left" Margin="14,10,0,0" VerticalAlignment="Top" Width="238" Height="33"/>
        <TextBlock Text="Accept this trainning?" HorizontalAlignment="Left" Margin="14,29,0,0" VerticalAlignment="Top" Width="238" Height="26"/>
    </Grid>
</Window>
