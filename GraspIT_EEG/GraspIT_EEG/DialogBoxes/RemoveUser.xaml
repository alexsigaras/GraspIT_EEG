<Window x:Class="GraspIT_EEG.DialogBoxes.RemoveUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Remove User?" Height="115" Width="285">
    <Grid Margin="0,0,-8,-7">
        <Button Width="100" Height="30" Content="Yes" IsDefault="True" Click="ButtonOkClick" Margin="14,0,0,13" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
        <Button Width="100" Height="30" Content="Cancel" IsCancel="True" Margin="0,0,31,13" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
        <Label Content="Are you sure you want to remove this user?" HorizontalAlignment="Left" Margin="14,10,0,0" VerticalAlignment="Top" Width="238"/>
    </Grid>
</Window>
