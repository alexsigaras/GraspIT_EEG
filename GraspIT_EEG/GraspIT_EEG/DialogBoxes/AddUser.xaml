<Window x:Class="GraspIT_EEG.DialogBoxes.AddUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add User" Height="133" Width="285">
    <Grid>
        <Button Width="100" Height="30" Content="OK" IsDefault="True" Click="ButtonOkClick" Margin="14,0,0,13" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
        <Button Width="100" Height="30" Content="Cancel" IsCancel="True" Margin="0,0,10,13" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
        <Label Content="User Name:" HorizontalAlignment="Left" Margin="14,10,0,0" VerticalAlignment="Top" Width="76"/>
        <TextBox x:Name="UserNameTxtBox" HorizontalAlignment="Left" Height="23" Margin="95,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="166"/>
    </Grid>
</Window>
