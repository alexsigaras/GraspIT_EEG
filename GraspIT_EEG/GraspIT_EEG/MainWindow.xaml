							<!-- Region Expressiv -->
							<TabItem Header="Expressiv">
								<Grid Margin="0,0,-0.01,0.424">
									<Grid.ColumnDefinitions>
										<ColumnDefinition/>
									</Grid.ColumnDefinitions>
									<Rectangle x:Name="ClenchRect"  Fill="Black" HorizontalAlignment="Left" Height="100" Margin="37,167,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
									<Rectangle x:Name="EyebrowRect"  Fill="Black" HorizontalAlignment="Left" Height="100" Margin="184,167,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
									<Label x:Name="Clench" Content="Label" HorizontalAlignment="Left" Margin="37,288,0,0" VerticalAlignment="Top"/>
									<Label x:Name="Eyebrows" Content="Label" HorizontalAlignment="Left" Margin="184,288,0,0" VerticalAlignment="Top"/>
									<CheckBox x:Name="ClenchCheckBox" Content="Clench" HorizontalAlignment="Left" Margin="63,38,0,0" VerticalAlignment="Top"/>
									<CheckBox x:Name="EyebrowsCheckBox" Content="Eyebrows" HorizontalAlignment="Left" Margin="63,64,0,0" VerticalAlignment="Top"/>
									<CheckBox x:Name="SmileCheckBox" Content="Smile" HorizontalAlignment="Left" Margin="63,86,0,0" VerticalAlignment="Top"/>
									<Rectangle x:Name="SmileRect"  Fill="Black" HorizontalAlignment="Left" Height="100" Margin="321,167,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
									<Label x:Name="Smile" Content="Label" HorizontalAlignment="Left" Margin="321,288,0,0" VerticalAlignment="Top"/>
									<Label x:Name="LowerFaceActionLbl"  FontFamily="Segoe WP" Content="Lower Face Action:" HorizontalAlignment="Left" Margin="37,403,0,0" VerticalAlignment="Top"/>
									<Label x:Name="LowerFaceAction" FontFamily="Segoe WP" Content="Value" HorizontalAlignment="Left" Margin="152,402,0,0" VerticalAlignment="Top"/>
									<Label x:Name="UpperFaceActionLbl" FontFamily="Segoe WP" Content="Upper Face Action:" HorizontalAlignment="Left" Margin="37,338,0,0" VerticalAlignment="Top"/>
									<Label x:Name="UpperFaceAction"  FontFamily="Segoe WP" Content="Value" HorizontalAlignment="Left" Margin="152,337,0,0" VerticalAlignment="Top"/>
									<Label x:Name="MiddleFaceActionLbl" FontFamily="Segoe WP" Content="Middle Face Action:" HorizontalAlignment="Left" Margin="37,367,0,0" VerticalAlignment="Top"/>
									<Label x:Name="MiddleFaceAction"  FontFamily="Segoe WP" Content="Value" HorizontalAlignment="Left" Margin="152,366,0,0" VerticalAlignment="Top"/>
									<Label x:Name="MiddleFaceActionRight"  FontFamily="Segoe WP" Content="Value" HorizontalAlignment="Left" Margin="258,366,0,0" VerticalAlignment="Top"/>
									<!--Expressiv Controls-->
								</Grid>
							</TabItem>

							<!-- EndRegion -->

							<!-- Region Affectiv -->

							<TabItem Header="Affectiv">
								<Grid>
									<telerik:RadCartesianChart x:Name="AffectivChart" Palette="Windows8" MaxZoom="100,1" Zoom="100,1" Margin="0,0,0,80">
										<telerik:RadCartesianChart.Behaviors>
											<telerik:ChartPanAndZoomBehavior PanMode="Both" ZoomMode="Both" />
											<telerik:ChartTrackBallBehavior ShowIntersectionPoints="True"
										ShowTrackInfo="True"
										SnapMode="AllClosePoints" />
										</telerik:RadCartesianChart.Behaviors>
										<telerik:RadCartesianChart.Grid>
											<telerik:CartesianChartGrid MajorXLineDashArray="5, 5" MajorYLineDashArray="5, 5" MajorLinesVisibility="XY">
												<telerik:CartesianChartGrid.MajorYLineStyle>
													<Style TargetType="{x:Type Line}">
														<Setter Property="Stroke" Value="Gray"/>
													</Style>
												</telerik:CartesianChartGrid.MajorYLineStyle>
												<telerik:CartesianChartGrid.MajorXLineStyle>
													<Style TargetType="{x:Type Line}">
														<Setter Property="Stroke" Value="Gray"/>
													</Style>
												</telerik:CartesianChartGrid.MajorXLineStyle>
											</telerik:CartesianChartGrid>
										</telerik:RadCartesianChart.Grid>
										<telerik:RadCartesianChart.VerticalAxis>
											<telerik:LinearAxis/>
										</telerik:RadCartesianChart.VerticalAxis>
										<telerik:RadCartesianChart.HorizontalAxis>
											<telerik:DateTimeContinuousAxis MajorStepUnit="Second" 
										LabelInterval="5"
										LabelFormat="HH:mm:ss.f" FontFamily="Segoe UI"
										PlotMode="OnTicks" />
										</telerik:RadCartesianChart.HorizontalAxis>
										<telerik:SplineSeries Stroke="Blue"/>
										<telerik:SplineSeries Stroke="Red"/>
										<telerik:SplineSeries Stroke="Orange"/>
										<telerik:SplineSeries Stroke="Black"/>
										<telerik:SplineSeries Stroke="Chocolate"/>
									</telerik:RadCartesianChart>
									<!--Affectiv Controls-->
								</Grid>
							</TabItem>

							<!-- EndRegion -->

							<!-- Region Cognitiv -->

							<TabItem Header="Cognitiv">
								<Grid>
									<Label x:Name="cognitivPower" Content="Label" HorizontalAlignment="Left" Margin="150,109,0,0" VerticalAlignment="Top"/>
									<Label x:Name="cognitivIsActive" Content="Label" HorizontalAlignment="Left" Margin="150,65,0,0" VerticalAlignment="Top"/>
									<Label x:Name="cognitivIsState" Content="Label" HorizontalAlignment="Left" Margin="150,145,0,0" VerticalAlignment="Top"/>
									<!--Cognitiv Controls-->
								</Grid>
							</TabItem>

							<!-- EndRegion -->

							<!-- Region Gyro -->

							<TabItem Header="Gyro">
								<Grid>
									<!--Gyro Controls-->
