                </ObjectAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="CognitivTrainingPageOut">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="CognitivTrainingPage">
                    <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
