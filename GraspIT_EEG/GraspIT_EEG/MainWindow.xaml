            <Storyboard x:Key="SSVEPPageOut"/>
            <Storyboard x:Key="CognitivPageIn"/>
            <Storyboard x:Key="CognitivPageOut"/>
            <Storyboard x:Key="EMGPageIn">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="EMGPage">
                    <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.5" Value="1"/>
