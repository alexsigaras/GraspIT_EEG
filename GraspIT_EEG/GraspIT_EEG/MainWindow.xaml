						<!--<Button Style="{DynamicResource MetroCircleButtonStyle}" FontFamily="Segoe UI Symbol" Content="&#xE112;" HorizontalAlignment="Left" Margin="146,222,0,0" VerticalAlignment="Top" Width="45" Height="45"/>-->
						<!--<Controls:RangeSlider Margin="255,110,143.209,148.178"></Controls:RangeSlider>-->
						<!--<Slider Margin="96,128,329.209,148.178"></Slider>-->
					</Grid>
				</TabItem>
				<TabItem Header="Cognitiv">
					<Grid>
						<!--Background="#FFE5E5E5"-->
						<!--<Button Style="{DynamicResource MetroCircleButtonStyle}" FontFamily="Segoe UI Symbol" Content="&#xE112;" HorizontalAlignment="Left" Margin="146,222,0,0" VerticalAlignment="Top" Width="45" Height="45"/>-->
					</Grid>
				</TabItem>
			</TabControl>


		</Grid>

		<!-- EndRegion -->

		<!-- Region Settings -->

		<Grid x:Name="SettingsPage">

			<!-- Region Settings Sidebar -->

			<Button x:Name="BackToMainWindowBtn" Style="{DynamicResource MetroCircleButtonStyle}" FontFamily="Segoe UI Symbol" FontSize="18" Content="&#xE112;" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="45" Height="45" Click="BackToMainWindowBtn_Click"/>
			<Label x:Name="SettingsLbl" Content="Settings" HorizontalAlignment="Left" Margin="60,13,0,0" VerticalAlignment="Top" FontSize="24"/>
