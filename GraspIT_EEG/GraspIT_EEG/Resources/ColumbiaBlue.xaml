<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!--ACCENT COLORS-->
	<Color x:Key="HighlightDarkColor">#FF086F9E</Color>
	<Color x:Key="HighlightLightColor">#FF086F9E</Color>
	
	<Color x:Key="AccentDarkColor">#FF1c739d</Color>
	
	<!--COLUMBIA COLORS-->
	<Color x:Key="HighlightDarkNightBlue">#FF192D4B</Color>
	<Color x:Key="HighlightLightNightBlue">#FF192D4B</Color>

	<Color x:Key="NightBlue">#CC192D4B</Color>
	<Color x:Key="RoyalBlue">#CC163A6F</Color>
	<Color x:Key="BannerBlue">#CC406AA5</Color>
	<Color x:Key="ColumbiaBlue">#CC8EB4E3</Color>

	<!--80%-->
	<!--<Color x:Key="AccentColor">#CC119EDA</Color>-->
	<Color x:Key="AccentColor">#CC406AA5</Color>
	<!--60%-->
	<Color x:Key="AccentColor2">#99119EDA</Color>
	<!--40%-->
	<Color x:Key="AccentColor3">#66119EDA</Color>
	<!--20%-->
	<Color x:Key="AccentColor4">#33119EDA</Color>

	<LinearGradientBrush x:Key="BrandingBrush"
						 EndPoint="0.001,0.5"
						 StartPoint="1.002,0.5">
		<GradientStop Color="{DynamicResource HighlightDarkColor}"
					  Offset="0" />
		<GradientStop Color="#FF5FC316"
					  Offset="1" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="AccentColorBrush"
					 Color="{StaticResource BannerBlue}" />

	<SolidColorBrush x:Key="AccentSelectedColorBrush"
					 Color="White" />


</ResourceDictionary>